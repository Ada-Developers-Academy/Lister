<table>
  <thead>
    <th>Name</th>
    <th>Email</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%=link_to "#{user.username}", user_path(user) %></td>
        <td><%=user.email%></td>   
      </tr>
    <% end %>
  </tbody>
</table>

<%= @users.all.inspect %>
<%= @users.all.count %>

