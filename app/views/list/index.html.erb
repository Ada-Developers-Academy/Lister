<h2>My Lists:</h2>
<!-- TODO: Add some javascript which collapses lists -->

<p><% @lists.each do |list| %>
        <%= link_to "#{list.title}", list_path(list) %><br>
        <%= link_to "Edit", edit_list_path(list) %></p>
        <%= link_to "Delete", list, method: :delete, class: "destroy", data: { confirm: 'Are you sure?' } %>
<% end %>
